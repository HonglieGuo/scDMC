# 忽略 Python 虚拟环境
/venv/
/.venv/
/env/
/.env/
/activate

# 忽略 Python 缓存文件和字节码
__pycache__
*.pyc
*.pyo
*.pyd

# 忽略 IDE 和编辑器配置文件
.idea/
.vscode/
*.swp

# 忽略 datasets 文件夹下的所有内容
/datasets/*
!/datasets/
!/datasets/**/README.md

# 对 models 文件夹应用同样的规则
/models/*
!/models/
!/models/**/README.md

# 忽略日志、构建和分发文件
*.log
/build/
/dist/
*.egg-info/